<?xml version="1.0" encoding="utf-8"?><RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"  xmlns:app="http://schemas.android.com/apk/res-auto"  android:layout_width="match_parent"  android:layout_height="match_parent"  android:orientation="vertical">  <LinearLayout    android:id="@+id/layout"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:orientation="vertical">    <android.support.v4.widget.NestedScrollView      android:id="@+id/scroll"      android:layout_width="match_parent"      android:layout_height="match_parent">      <LinearLayout        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:orientation="vertical">        <LinearLayout          android:layout_width="match_parent"          android:layout_height="wrap_content"          android:padding="@dimen/dp_5"          android:background="@color/white"          android:orientation="vertical">          <com.xuexiang.xui.widget.textview.supertextview.SuperTextView            android:layout_width="match_parent"            android:layout_height="35dp"            app:sDividerLineType="none"            app:sLeftTextIsBold="true"            app:sLeftTextSize="16sp"            app:sLeftTextString="租车信息:"            app:sLeftTvDrawableLeft="@drawable/ic_zulinhetong"            app:sTextViewDrawablePadding="5dp"></com.xuexiang.xui.widget.textview.supertextview.SuperTextView>          <RelativeLayout            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:paddingTop="@dimen/dp_5"            android:paddingBottom="@dimen/dp_5"            android:paddingLeft="@dimen/dp_10"            android:paddingRight="@dimen/dp_10"            >            <hzkj.cc.yw.BoldTextView              android:layout_width="wrap_content"              android:layout_height="wrap_content"              android:layout_centerVertical="true"              android:text="用车时间"              android:textColor="@color/black"              android:textSize="@dimen/sp_14"/>            <TextView              android:id="@+id/useStartTime"              android:layout_width="match_parent"              android:layout_height="wrap_content"              android:layout_centerVertical="true"              android:gravity="right"              android:textColor="@color/black"              android:textSize="@dimen/sp_14"              />          </RelativeLayout>          <RelativeLayout            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:paddingTop="@dimen/dp_5"            android:paddingBottom="@dimen/dp_5"            android:paddingLeft="@dimen/dp_10"            android:paddingRight="@dimen/dp_10"            android:background="@color/white">            <hzkj.cc.yw.BoldTextView              android:layout_width="wrap_content"              android:layout_height="wrap_content"              android:layout_centerVertical="true"              android:text="用车项目"              android:textColor="@color/black"              android:textSize="@dimen/sp_14"/>            <TextView              android:id="@+id/project"              android:layout_width="match_parent"              android:layout_height="wrap_content"              android:layout_centerVertical="true"              android:gravity="right"              android:textColor="@color/black"              android:textSize="@dimen/sp_14"              />          </RelativeLayout>          <RelativeLayout            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:paddingTop="@dimen/dp_5"            android:paddingBottom="@dimen/dp_5"            android:paddingLeft="@dimen/dp_10"            android:paddingRight="@dimen/dp_10"            android:background="@color/white">            <hzkj.cc.yw.BoldTextView              android:layout_width="wrap_content"              android:layout_height="wrap_content"              android:layout_centerVertical="true"              android:text="人员组成"              android:textColor="@color/black"              android:textSize="@dimen/sp_14"/>            <TextView              android:id="@+id/persons"              android:layout_width="match_parent"              android:layout_height="wrap_content"              android:layout_centerVertical="true"              android:gravity="right"              android:textColor="@color/black"              android:textSize="@dimen/sp_14"              />          </RelativeLayout>          <RelativeLayout            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:paddingTop="@dimen/dp_5"            android:paddingBottom="@dimen/dp_5"            android:paddingLeft="@dimen/dp_10"            android:paddingRight="@dimen/dp_10"            android:background="@color/white">            <hzkj.cc.yw.BoldTextView              android:layout_width="wrap_content"              android:layout_height="wrap_content"              android:layout_centerVertical="true"              android:text="用车事由"              android:textColor="@color/black"              android:textSize="@dimen/sp_14"/>            <TextView              android:id="@+id/reason"              android:layout_width="match_parent"              android:layout_height="wrap_content"              android:layout_centerVertical="true"              android:gravity="right"              android:textColor="@color/black"              android:textSize="@dimen/sp_14"              />          </RelativeLayout>        </LinearLayout>        <LinearLayout          android:layout_width="match_parent"          android:layout_height="wrap_content"          android:layout_marginTop="@dimen/dp_15"          android:padding="@dimen/dp_5"          android:background="@color/white"          android:orientation="vertical">          <com.xuexiang.xui.widget.textview.supertextview.SuperTextView            android:layout_width="match_parent"            android:layout_height="35dp"            app:sDividerLineType="none"            app:sLeftTextIsBold="true"            app:sLeftTextSize="18sp"            app:sLeftTextString="车辆信息:"            app:sLeftTvDrawableLeft="@drawable/ic_cheliangguanli"            app:sTextViewDrawablePadding="5dp"></com.xuexiang.xui.widget.textview.supertextview.SuperTextView>          <RelativeLayout            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:paddingTop="@dimen/dp_5"            android:paddingBottom="@dimen/dp_5"            android:paddingLeft="@dimen/dp_10"            android:paddingRight="@dimen/dp_10"            >            <hzkj.cc.yw.BoldTextView              android:layout_width="wrap_content"              android:layout_height="wrap_content"              android:layout_centerVertical="true"              android:text="车主姓名"              android:textColor="@color/black"              android:textSize="@dimen/sp_14"/>            <TextView              android:id="@+id/carOwnerName"              android:layout_width="match_parent"              android:layout_height="wrap_content"              android:layout_centerVertical="true"              android:gravity="right"              android:textColor="@color/black"              android:textSize="@dimen/sp_14"              />          </RelativeLayout>          <RelativeLayout            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:paddingTop="@dimen/dp_5"            android:paddingBottom="@dimen/dp_5"            android:paddingLeft="@dimen/dp_10"            android:paddingRight="@dimen/dp_10"            android:background="@color/white">            <hzkj.cc.yw.BoldTextView              android:layout_width="wrap_content"              android:layout_height="wrap_content"              android:layout_centerVertical="true"              android:text="车主联系电话"              android:textColor="@color/black"              android:textSize="@dimen/sp_14"/>            <TextView              android:id="@+id/carOwnerTel"              android:layout_width="match_parent"              android:layout_height="wrap_content"              android:layout_centerVertical="true"              android:gravity="right"              android:textColor="@color/black"              android:textSize="@dimen/sp_14"              />          </RelativeLayout>          <RelativeLayout            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:paddingTop="@dimen/dp_5"            android:paddingBottom="@dimen/dp_5"            android:paddingLeft="@dimen/dp_10"            android:paddingRight="@dimen/dp_10"            android:background="@color/white">            <hzkj.cc.yw.BoldTextView              android:layout_width="wrap_content"              android:layout_height="wrap_content"              android:layout_centerVertical="true"              android:text="车型"              android:textColor="@color/black"              android:textSize="@dimen/sp_14"/>            <TextView              android:id="@+id/carType"              android:layout_width="match_parent"              android:layout_height="wrap_content"              android:layout_centerVertical="true"              android:gravity="right"              android:textColor="@color/black"              android:textSize="@dimen/sp_14"              />          </RelativeLayout>          <RelativeLayout            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:paddingTop="@dimen/dp_5"            android:paddingBottom="@dimen/dp_5"            android:paddingLeft="@dimen/dp_10"            android:paddingRight="@dimen/dp_10"            android:background="@color/white">            <hzkj.cc.yw.BoldTextView              android:layout_width="wrap_content"              android:layout_height="wrap_content"              android:layout_centerVertical="true"              android:text="车牌"              android:textColor="@color/black"              android:textSize="@dimen/sp_14"/>            <TextView              android:id="@+id/carLicense"              android:layout_width="match_parent"              android:layout_height="wrap_content"              android:layout_centerVertical="true"              android:gravity="right"              android:textColor="@color/black"              android:textSize="@dimen/sp_14"              />          </RelativeLayout>        </LinearLayout>        <LinearLayout          android:layout_width="match_parent"          android:layout_height="wrap_content"          android:layout_marginTop="@dimen/dp_15"          android:padding="@dimen/dp_5"          android:background="@color/white"          android:orientation="vertical">          <com.xuexiang.xui.widget.textview.supertextview.SuperTextView            android:layout_width="match_parent"            android:layout_height="35dp"            app:sDividerLineType="none"            app:sLeftTextIsBold="true"            app:sLeftTextSize="@dimen/sp_18"            app:sLeftTextString="费用信息:"            app:sLeftTvDrawableLeft="@drawable/ic_price"            app:sTextViewDrawablePadding="5dp"></com.xuexiang.xui.widget.textview.supertextview.SuperTextView>          <RelativeLayout            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:paddingTop="@dimen/dp_5"            android:paddingBottom="@dimen/dp_5"            android:paddingLeft="@dimen/dp_10"            android:paddingRight="@dimen/dp_10"            >            <hzkj.cc.yw.BoldTextView              android:layout_width="wrap_content"              android:layout_height="wrap_content"              android:layout_centerVertical="true"              android:text="租车价格(元/天)"              android:textColor="@color/black"              android:textSize="@dimen/sp_14"/>            <TextView              android:id="@+id/perDayPrice"              android:layout_width="match_parent"              android:layout_height="wrap_content"              android:layout_centerVertical="true"              android:gravity="right"              android:textColor="@color/black"              android:textSize="@dimen/sp_14"              />          </RelativeLayout>          <RelativeLayout            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:paddingTop="@dimen/dp_5"            android:paddingBottom="@dimen/dp_5"            android:paddingLeft="@dimen/dp_10"            android:paddingRight="@dimen/dp_10"            android:background="@color/white">            <hzkj.cc.yw.BoldTextView              android:layout_width="wrap_content"              android:layout_height="wrap_content"              android:layout_centerVertical="true"              android:text="租车天数"              android:textColor="@color/black"              android:textSize="@dimen/sp_14"/>            <TextView              android:id="@+id/days"              android:layout_width="match_parent"              android:layout_height="wrap_content"              android:layout_centerVertical="true"              android:gravity="right"              android:textColor="@color/black"              android:textSize="@dimen/sp_14"              />          </RelativeLayout>          <RelativeLayout            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:paddingTop="@dimen/dp_5"            android:paddingBottom="@dimen/dp_5"            android:paddingLeft="@dimen/dp_10"            android:paddingRight="@dimen/dp_10"            android:background="@color/white">            <hzkj.cc.yw.BoldTextView              android:layout_width="wrap_content"              android:layout_height="wrap_content"              android:layout_centerVertical="true"              android:text="费用合计(元)"              android:textColor="@color/black"              android:textSize="@dimen/sp_14"/>            <TextView              android:id="@+id/totalMoney"              android:layout_width="match_parent"              android:layout_height="wrap_content"              android:layout_centerVertical="true"              android:gravity="right"              android:textColor="@color/black"              android:textSize="@dimen/sp_14"              />          </RelativeLayout>          <RelativeLayout            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:paddingTop="@dimen/dp_5"            android:paddingBottom="@dimen/dp_5"            android:paddingLeft="@dimen/dp_10"            android:paddingRight="@dimen/dp_10"            android:background="@color/white">            <hzkj.cc.yw.BoldTextView              android:layout_width="wrap_content"              android:layout_height="wrap_content"              android:layout_centerVertical="true"              android:text="费用构成"              android:textColor="@color/black"              android:textSize="@dimen/sp_14"/>            <TextView              android:id="@+id/totalMoneyComposition"              android:layout_width="match_parent"              android:layout_height="wrap_content"              android:layout_centerVertical="true"              android:gravity="right"              android:textColor="@color/black"              android:textSize="@dimen/sp_14"              />          </RelativeLayout>        </LinearLayout>        <LinearLayout          android:id="@+id/procedurePart"          android:layout_width="match_parent"          android:layout_height="wrap_content"          android:layout_marginTop="@dimen/dp_15"          android:padding="@dimen/dp_5"          android:background="@color/white"          android:orientation="vertical">          <com.xuexiang.xui.widget.textview.supertextview.SuperTextView            android:id="@+id/displayError"            android:layout_width="match_parent"            android:layout_height="35dp"            app:sDividerLineType="none"            app:sLeftTextIsBold="true"            app:sLeftTextSize="18sp"            app:sLeftTextString="审核相关:"            app:sLeftTvDrawableLeft="@drawable/ic_shenheyijian"            app:sTextViewDrawablePadding="5dp"></com.xuexiang.xui.widget.textview.supertextview.SuperTextView>          <RelativeLayout            android:id="@+id/procedureManPart"            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:paddingTop="@dimen/dp_5"            android:paddingBottom="@dimen/dp_5"            android:paddingLeft="@dimen/dp_10"            android:paddingRight="@dimen/dp_10"            android:background="@color/white">            <hzkj.cc.yw.BoldTextView              android:layout_width="wrap_content"              android:layout_height="wrap_content"              android:layout_centerVertical="true"              android:text="审核人"              android:textColor="@color/black"              android:textSize="@dimen/sp_14"/>            <TextView              android:id="@+id/procedureMan"              android:layout_width="match_parent"              android:layout_height="wrap_content"              android:layout_centerVertical="true"              android:gravity="right"              android:textColor="@color/black"              android:textSize="@dimen/sp_14"              />          </RelativeLayout>          <RelativeLayout            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:paddingTop="@dimen/dp_5"            android:paddingBottom="@dimen/dp_5"            android:paddingLeft="@dimen/dp_10"            android:paddingRight="@dimen/dp_10"            android:background="@color/white">            <hzkj.cc.yw.BoldTextView              android:layout_width="wrap_content"              android:layout_height="wrap_content"              android:layout_centerVertical="true"              android:text="审核意见"              android:textColor="@color/black"              android:textSize="@dimen/sp_14"/>            <TextView              android:layout_width="match_parent"              android:layout_height="wrap_content"              android:layout_centerVertical="true"              android:gravity="right"              android:textColor="@color/black"              android:textSize="@dimen/sp_14"              />          </RelativeLayout>          <hzkj.cc.yw.BoldTextView            android:id="@+id/displayReason"            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:layout_margin="10dp"            android:textSize="@dimen/sp_14"/>          <LinearLayout            android:id="@+id/picPart"            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:orientation="vertical">            <RelativeLayout              android:id="@+id/procedurePicPart"              android:layout_width="match_parent"              android:layout_height="wrap_content"              android:paddingTop="@dimen/dp_5"              android:paddingBottom="@dimen/dp_5"              android:paddingLeft="@dimen/dp_10"              android:paddingRight="@dimen/dp_10"              android:background="@color/white">              <hzkj.cc.yw.BoldTextView                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_centerVertical="true"                android:text="审核图片"                android:textColor="@color/black"                android:textSize="@dimen/sp_14"/>            </RelativeLayout>            <LinearLayout              android:layout_width="match_parent"              android:layout_height="wrap_content"              android:padding="@dimen/dp_10">              <LinearLayout                android:layout_width="@dimen/dp_0"                android:layout_height="wrap_content"                android:layout_weight="1"                android:gravity="center">                <ImageView                  android:id="@+id/pic1"                  android:layout_width="72dp"                  android:layout_height="72dp"                  android:src="@drawable/upload"/>              </LinearLayout>              <LinearLayout                android:layout_width="@dimen/dp_0"                android:layout_height="wrap_content"                android:layout_weight="1"                android:gravity="center">                <ImageView                  android:id="@+id/pic2"                  android:layout_width="72dp"                  android:layout_height="72dp"                  android:src="@drawable/upload"/>              </LinearLayout>            </LinearLayout>          </LinearLayout>        </LinearLayout>      </LinearLayout>    </android.support.v4.widget.NestedScrollView>  </LinearLayout></RelativeLayout>